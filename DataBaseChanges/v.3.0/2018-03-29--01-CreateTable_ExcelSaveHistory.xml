<?xml version="1.0" encoding="utf-8" ?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

  <changeSet id="CreateTable_ExcelSaveHistory" author="Ura">

    <createTable tableName="dsh_ExcelSaveHistory">

      <column name="id"  type="INT" autoIncrement="true">
        <constraints nullable="false" primaryKey="true"/>
      </column>

      <column name="user_Name" type="nvarchar(100)"/>

      <column name="fileName" type="nvarchar(255)"/>

      <column name="ReportName" type="nvarchar(100)"/>

      <column name="hst_datetime" type="datetime"/>

    </createTable>

  </changeSet>
</databaseChangeLog>