<?xml version="1.0" encoding="utf-8" ?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

  <changeSet id="Create_ent_ContactSalarySales_Table" author="Ura">

    <createTable tableName="ent_ContactSalarySales">

      <column name="id"  type="INT" autoIncrement="true">
        <constraints nullable="false" primaryKey="true"/>
      </column>

      <column name="contact_id" type="INT">
        <constraints nullable="false" />
      </column>

      <column name="month" type="DATE">
        <constraints nullable="false" />
      </column>

      <column name="SalarySales" type="DECIMAL(18,2)">
      </column>

    </createTable>
  </changeSet>
</databaseChangeLog>
